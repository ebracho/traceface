<!doctype html>
<html>
	<head>
		<title>traceface</title>
		<link rel="stylesheet" href="style.css">
	</head>
	<body>
		{% for frame in trace %}
			<div class="frame">
				<b>{{frame.func_name}}</b>
				({{ frame.filepath }}:{{ frame.line_no}})
				<code>{{ frame.rendered }}</code>
			</div>
		{% endfor %}
	</body>
</html>
